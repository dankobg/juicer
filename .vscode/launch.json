{
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Web launch chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:3974",
      "webRoot": "${workspaceFolder}/web"
    },
    {
      "name": "Go launch package",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "showLog": true,
      "trace": "verbose"
    },
    {
      "name": "Go launch file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}",
      "showLog": true,
      "trace": "verbose"
    },
    {
      "name": "Go attach docker",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "remotePath": "/app",
      "port": 2345,
      "host": "127.0.0.1",
      "showLog": true,
      "trace": "verbose"
    }
  ]
}
