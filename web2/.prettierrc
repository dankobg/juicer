{
	"endOfLine": "lf",
	"useTabs": true,
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 120,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"arrowParens": "avoid",
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
